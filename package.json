{
  "name": "voce",
  "version": "0.0.1",
  "description": "API test runner",
  "main": "dist/index.js",
  "bin": {
    "voce": "cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "cross-env DEBUG=voce ava -c 1 test/**/*.spec.ts",
    "coverage": "nyc npm test",
    "postcoverage": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotvirus/typescript-eslint-ava-template.git"
  },
  "keywords": [],
  "author": "dotvirus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dotvirus/typescript-eslint-ava-template/issues"
  },
  "homepage": "https://github.com/dotvirus/typescript-eslint-ava-template#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.11.2",
    "@types/ora": "^3.2.0",
    "@types/sinon": "^9.0.8",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "ava": "^3.13.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "@dotvirus/yxc": "^2.0.2",
    "chalk": "^4.1.0",
    "debug": "^4.2.0",
    "haxan": "0.0.1-nightly.4",
    "ora": "^5.1.0",
    "sinon": "^9.2.0",
    "variable-diff": "^2.0.2",
    "yargs": "^16.0.3"
  }
}
